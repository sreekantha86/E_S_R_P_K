@model ems.domain.ViewModels.ServiceIncome

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="box box-danger">
    <div class="box-header with-border">
        <h3 class="box-title">Service Invoice - Create</h3>
    </div>
    <div class="box-body">
        <div class="row">
            <div class="col-xs-2">
                @Html.LabelFor(m => m.HeadModel.SerInvNo)
                <div class="row">
                    @Html.TextBoxFor(model => model.HeadModel.SerInvNo, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.HeadModel.SerInvNo)
                </div>
            </div>
            <div class="col-xs-4">
                <input type="text" class="form-control" placeholder=".col-xs-4">
            </div>
            <div class="col-xs-5">
                <input type="text" class="form-control" placeholder=".col-xs-5">
            </div>
        </div>
    </div>
    <!-- /.box-body -->
</div>
<!-- /.box -->

