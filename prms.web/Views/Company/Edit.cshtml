@model ems.domain.Company
@{
    ViewBag.Title = "Compay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="box">
    <div class="box-header">
        <h3 class="box-title">Company Details</h3>
    </div>
    @using (Html.BeginForm(null, null, new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { role = "form" }))
    {
        @Html.AntiForgeryToken()
        <!-- /.box-header -->
        <div class="box-body">
            <div class="box-body">
                @Html.Partial("_form", Model)
                <div class="box-footer">
                    <button type="button" class="btn btn-default" onclick="GotoHome()">Cancel</button>
                    <button type="submit" class="btn btn-info pull-right">Save</button>
                </div>
            </div>
        </div>
        <!-- /.box-body -->
    }
</div>

